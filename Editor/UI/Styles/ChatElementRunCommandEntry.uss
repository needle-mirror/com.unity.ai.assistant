.chatelementruncommandentry__element {
    background-color: var(--muse-chat-backgrounds-200);
    margin-top: 1px;
}

.preview-entry-container {
    flex-direction: row;
    flex-grow: 1;
    flex-wrap: wrap;
    min-height: 20px;
    margin: 4px 4px 4px 8px;
}

.preview-entry-description {
    flex-direction: row;
    flex-grow: 1;
    flex-wrap: wrap;
    min-height: 20px;
}

.entry-label-item {
    padding: 0;
    margin: 1px 0 0 0;
    -unity-text-align: middle-left;
    align-self: center;
    white-space: normal;
    height: 20px;
}

.entry-label-item Label {
    font-size: var(--muse-chat-small-font-size);
    margin: 1px;
}

.preview-entry-action-button {
    height: 14px;
    border-width: 0;
    display: flex;
    margin-right: 8px;
    align-self: center;
}
