.routespopup__element {
    position: absolute;
    left: 16px;
    max-width: 400px;
    right: 14px;
    justify-content: flex-end;
    border-radius: 2px;
}

.mui-routes-popup-root {
    background-color: var(--muse-chat-backgrounds-200);
    flex-direction: column;
    flex-shrink: 1;
}


.mui-first-popup-item {
    border-top-left-radius: 2px;
    border-top-right-radius: 2px;
}

.mui-last-popup-item {
    border-bottom-left-radius: 2px;
    border-bottom-right-radius: 2px;
}

.mui-routes-popup-no-results {
    padding: 6px;
    border-radius: 2px;
}


